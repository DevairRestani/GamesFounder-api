{
  "name": "GamesFounder-api",
  "version": "0.0.1",
  "main": "dist/server.js",
  "repository": "https://github.com/DevairRestani/GamesFounder-api.git",
  "author": "Devair Restani",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev --inspect --unhandled-rejections=strict --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "ts-node src/index.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "model:run": "typeorm-model-generator -h localhost -d GamesFounderDB -u postgres -x 102155545Ab. -e postgres -o ./src/models/ -s public"
  },
  "dependencies": {
    "@types/reflect-metadata": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "pg": "^8.3.3",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typeorm-model-generator": "^0.4.3",
    "typescript": "^4.0.3"
  }
}
